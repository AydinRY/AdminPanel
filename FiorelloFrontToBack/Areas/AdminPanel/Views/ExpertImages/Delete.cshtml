@model ExpertImages

@{
    ViewData["Title"] = "Delete";
}

<div class="row">
    <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
        <div class="card-body">
            <h4 class="card-title">Delete Expert</h4>
            <img src="~/img/@Model.Name" style="width: 150px; height: 150px;"/>
             <p class="text-danger">Are you sure want to delete?</p>
        </div>
        </div>
    </div>
</div>
<div class="row">
    <form method="post" class="forms-sample">
        <button type="submit" class="btn btn-danger">Delete</button>
    </form>
    <a class="btn btn-light" asp-action="Index">Cancel</a>
</div>
