@model ExpertImages

@{
    ViewData["Title"] = "Details";
}


<div class="row">
  <div class="col-md-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">@Model.Name</h4>
        <p>
          @Html.Raw(Model.Position)
        </p>
        <img src="~/img/@Model.Image" alt="image" class="profile-pic">
      </div>
    </div>
  </div>
</div>
